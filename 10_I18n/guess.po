#: /home/iter/hw/10_I18n/guess.c:35
msgid "Hello! Choose a number, I'll try to guess it."
msgstr "Добро пожаловать! Выберите число, я постараюсь его угадать."

#: /home/iter/hw/10_I18n/guess.c:41
#, c-format
msgid "Is your number larger than %d?\n"
msgstr "Ваше число больше, чем %d?\n"

#: /home/iter/hw/10_I18n/guess.c:24
msgid "Please enter \"yes\" or \"no\"\n"
msgstr "Введите \"да\" или \"нет\"\n"

#: /home/iter/hw/10_I18n/guess.c:52
msgid "Your number is"
msgstr "Ваше число —"

#: /home/iter/hw/10_I18n/guess.c:21
msgid "no"
msgstr "нет"

#: /home/iter/hw/10_I18n/guess.c:19
msgid "yes"
msgstr "да"
