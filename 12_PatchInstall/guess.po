#: /home/iter/hw/11_Documenting/guess.c:168
msgid "A number divination program that reads your mind."
msgstr "Предсказание загаданного числа через телекинез."

#: /home/iter/hw/11_Documenting/guess.c:181
msgid "Hello! Choose a number from 1 to 3999, I'll try to guess it."
msgstr "Добро пожаловать! Выберите число от 1 до 3999, я постараюсь его угадать."

#: /home/iter/hw/11_Documenting/guess.c:194
#, c-format
msgid "Is your number larger than %s?\n"
msgstr "Ваше число больше, чем %s?\n"

#: /home/iter/hw/11_Documenting/guess.c:126
msgid "Please enter \"yes\" or \"no\"\n"
msgstr "Введите \"да\" или \"нет\"\n"

#: /home/iter/hw/11_Documenting/guess.c:171
msgid "Print numbers in Roman numeric system"
msgstr "Выводить числа в Римской системе счисления"

#: /home/iter/hw/11_Documenting/guess.c:212
msgid "Your number is"
msgstr "Ваше число —"

#: /home/iter/hw/11_Documenting/guess.c:123
msgid "no"
msgstr "нет"

#: /home/iter/hw/11_Documenting/guess.c:121
msgid "yes"
msgstr "да"
